    "description": "Express REST API with PostgreSQL and TypeSCript",
    "type": "module",
    "module": "./dist/index.js",
    "main": "./dist/index.js",
    "files": [
        "./dist"
    ],
    "scripts": {
        "start": "tsc && node ./dist/config/dbConfig.js",
        "test": "echo \"Error: no test specified\" && exit 1",
